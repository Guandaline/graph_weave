{
  "__inputs": [],
  "__requires": [],
  "annotations": {
    "list": []
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Request Rate",
      "type": "timeseries",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job=\"graph\"}[5m]))",
          "legendFormat": "Total"
        },
        {
          "expr": "sum(rate(http_requests_total{job=\"graph\"}[5m])) by (method)",
          "legendFormat": "{{method}}"
        }
      ]
    },
    {
      "title": "Error Rate (5xx)",
      "type": "timeseries",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job=\"graph\", status_code=~\"5..\"}[5m]))",
          "legendFormat": "Errors"
        }
      ],
      "alert": {
        "conditions": [
          {
            "evaluator": {
              "params": [0.1],
              "type": "gt"
            },
            "query": {
              "params": ["A", "5m", "now"]
            },
            "reducer": {
              "params": [],
              "type": "avg"
            },
            "type": "query"
          }
        ],
        "for": "1m",
        "frequency": "1m",
        "handler": 1,
        "name": "High Error Rate Alert",
        "noDataState": "ok",
        "notifications": []
      }
    },
    {
      "title": "Request Duration (p95)",
      "type": "timeseries",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=\"graph\"}[5m])) by (le))",
          "legendFormat": "p95"
        }
      ],
      "unit": "s"
    },
    {
      "title": "Cache Hit Ratio (%)",
      "type": "gauge",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "(sum(rate(cache_hits_total{job=\"graph\"}[5m])) / (sum(rate(cache_hits_total{job=\"graph\"}[5m])) + sum(rate(cache_misses_total{job=\"graph\"}[5m])))) * 100",
          "legendFormat": "Overall"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "orange",
                "value": 80
              },
              {
                "color": "green",
                "value": 95
              }
            ]
          },
          "mappings": [],
          "color": {
            "mode": "thresholds"
          }
        }
      }
    }
  ],
  "schemaVersion": 35,
  "style": "dark",
  "tags": [],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "",
  "title": "Graph - Overview",
  "version": 1
}
