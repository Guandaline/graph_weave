  "title": "Hybrid RAG Overview",
  "version": 1,
  "panels": [
    {
      "type": "stat",
      "title": "Requests (5m)",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "sum(increase(rag_requests_total[5m]))"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 0,
        "y": 0
      }
    },
    {
      "type": "stat",
      "title": "Cache Hits (5m)",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "sum(increase(rag_cache_hits_total[5m]))"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 8,
        "y": 0
      }
    },
    {
      "type": "timeseries",
      "title": "Stage Latency p50",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum(rate(rag_stage_latency_seconds_bucket[5m])) by (le,stage))",
          "legendFormat": "{{stage}}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 6
      }
    },
    {
      "type": "timeseries",
      "title": "Total Latency p50",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum(rate(rag_total_latency_seconds_bucket[5m])) by (le))"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 6
      }
    }
  ]
